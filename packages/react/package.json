{
  "name": "@reactify/react",
  "version": "0.0.0",
  "private": false,
  "description": "A library for archiving the useful react components, hooks or utils so on.",
  "author": "Jeongjin Oh <rojay.developer@gmail.com>",
  "exports": {
    ".": {
